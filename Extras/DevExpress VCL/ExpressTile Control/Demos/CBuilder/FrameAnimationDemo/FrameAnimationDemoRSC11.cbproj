<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <MainSource>FrameAnimationDemo.cpp</MainSource>
    <Config Condition="'$(Config)'==''">Base</Config>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Config)'=='Base' or '$(Base)'!=''">
    <Base>true</Base>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Base)'!=''">
    <DCC_CBuilderOutput>JPHNE</DCC_CBuilderOutput>
    <Defines>NO_STRICT</Defines>
    <ProjectType>CppVCLApplication</ProjectType>
    <BCC_wpar>false</BCC_wpar>
    <IncludePath>$(BDS)\include;$(BDS)\include\dinkumware;$(BDS)\include\vcl;..\Common;..</IncludePath>
    <AllPackageLibs>rtl.lib;vcl.lib;vcldb.lib;dbrtl.lib;bdertl.lib;adortl.lib;dxCoreRS11.lib;cxLibraryRS11.lib;dxGDIPlusRS11.lib;dxThemeRS11.lib;dsnap.lib;cxEditorsRS11.lib;cxDataRS11.lib;vcljpg.lib;vclx.lib;dxCoreRS11.lib;cxPageControlRS11.lib;dxTileControlRS11.lib;dxSkinsCoreRS11.lib;cxExportRS11.lib;cxGridRS11.lib;cxPivotGridRS11.lib;dxmdsRS11.lib;psapi.lib;dxLayoutControlRS11.lib;dxComnRS11.lib</AllPackageLibs>
    <ILINK_LibraryPath>$(BDS)\lib\release;$(BDS)\lib;$(BDS)\lib\obj;$(BDS)\lib\psdk;..\Common;..</ILINK_LibraryPath>
  </PropertyGroup>
  <ProjectExtensions>
    <Borland.Personality>CPlusPlusBuilder.Personality</Borland.Personality>
    <Borland.ProjectType>CppVCLApplication</Borland.ProjectType>
    <BorlandProject>
      <BorlandProject>
        <CPlusPlusBuilder.Personality>
        </CPlusPlusBuilder.Personality>
      </BorlandProject>
    </BorlandProject>
  </ProjectExtensions>
  <Import Project="$(MSBuildBinPath)\Borland.Cpp.Targets" />
  <ItemGroup>
    <ResFiles Include="FrameAnimationDemo.res">
    </ResFiles>
    <CppCompile Include="FrameAnimationDemo.cpp"> </CppCompile>
    <CppCompile Include="FrameAnimationDemoDM.cpp">
      <Form>DM</Form>
    </CppCompile>
    <CppCompile Include="FrameAnimationDemoMain.cpp">
      <Form>fmFrameAnimationMain</Form>
    </CppCompile>
    <CppCompile Include="..\Common\DemoUtils.cpp"> </CppCompile>
    <CppCompile Include="..\Common\AboutDemoForm.cpp">
      <Form>formAboutDemo</Form>
    </CppCompile>
    <FormResources Include="FrameAnimationDemoDM.dfm" />
    <FormResources Include="FrameAnimationDemoMain.dfm" />
    <FormResources Include="..\Common\AboutDemoForm.dfm" />
  </ItemGroup>
</Project>