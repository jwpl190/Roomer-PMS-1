#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = EBar2000Demo.exe BackgroundDemo.exe DockingMegaDemo.exe InPlaceEditorsDemo.exe \
BarNotepadDemo.exe RibbonNotepadDemo.exe SampleDocking.exe NotesDemo.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------


EBar2000Demo.exe: EBar2000Demo\EBar2000Demo.dpr
  $(DCC)

BackgroundDemo.exe: BackgroundDemo\BackgroundDemo.dpr
  $(DCC)

DockingMegaDemo.exe: DockingMegaDemo\DockingMegaDemo.dpr
  $(DCC)

InPlaceEditorsDemo.exe: InPlaceEditorsDemo\InPlaceEditorsDemo.dpr
  $(DCC)

BarNotepadDemo.exe: BarNotepadDemo\BarNotepadDemo.dpr
  $(DCC)

RibbonNotepadDemo.exe: RibbonNotepadDemo\RibbonNotepadDemo.dpr
  $(DCC)

SampleDocking.exe: SampleDocking\SampleDocking.dpr
  $(DCC)

NotesDemo.exe: NotesDemo\NotesDemo.dpr
  $(DCC)
